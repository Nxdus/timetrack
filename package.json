{
  "name": "timetrack",
  "displayName": "timetrack",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.108.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "timetrack.openStats",
        "title": "TimeTrack: Open Stats"
      },
      {
        "command": "timetrack.resetData",
        "title": "TimeTrack: Reset All Data"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run webview:build && webpack",
    "watch": "webpack --watch",
    "package": "npm run webview:build && webpack --mode production --devtool hidden-source-map",
    "webview:install": "npm --prefix webview-ui install",
    "webview:build": "npm --prefix webview-ui run build",
    "webview:dev": "npm --prefix webview-ui run dev",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.108.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.52.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "ts-loader": "^9.5.4",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
